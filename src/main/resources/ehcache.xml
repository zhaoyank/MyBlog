<?xml version="1.0" encoding="UTF-8" ?>
<ehcache>
	<diskStore path="java.io.tmpdir" />
	<cache name="node"
		maxElementsInMemory="1000"
		eternal="false"
		timeToIdleSeconds="300"
		timeToLiveSeconds="300"
		overflowToDisk="true"
	/>
		
	<cache name="article"
		maxElementsInMemory="100"
		eternal="false"
		timeToIdleSeconds="300"
		timeToLiveSeconds="300"
		overflowToDisk="true"
	/>
</ehcache>